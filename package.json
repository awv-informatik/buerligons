{
  "name": "buerligons",
  "homepage": "./",
  "version": "0.0.1",
  "author": "AWV Informatik AG <info@awv-informatik.ch>",
  "repository": "https://github.com/awv-informatik/buerligons",
  "private": true,
  "scripts": {
    "clean": "rimraf ./build",
    "start": "npm run dev",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "react-app-rewired start",
    "compile": "tsc",
    "lint": "eslint \"./src/**/*.ts?(x)\"",
    "classcad": "classcad --ip localhost --port 8182 --instances 5 --ccappfile ./BaseModeling.ccapp"
  },
  "engines": {
    "node": ">=14 <17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "avoid",
    "jsxBracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@buerli.io/react-cad": "0.4.1",
    "@classcad/windows-x64": "0.0.0-beta.15",
    "@react-three/drei": "8.20.1",
    "@react-three/fiber": "7.0.19",
    "@react-three/postprocessing": "2.3.1",
    "antd": "4.16.13",
    "arraybuffer-loader": "1.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "three": "0.138.0",
    "zustand": "3.5.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "@types/three": "0.137.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "customize-cra-react-refresh": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "4.4.3",
    "url-loader": "^4.1.1",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
